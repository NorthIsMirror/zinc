# vim: ft=zsh ts=2 sw=2 et fenc=utf-8

p10ks_retval ()
{
  if (( _P10K_RETVAL_STORE == 0 )); then
    REPLY='âœ”'
  elif (( _P10K_RETVAL_STORE <= 128 )); then
    REPLY="${_P10K_RETVAL_STORE}"
  else
    local sig=$(( _P10K_RETVAL_STORE - 128 ))
    local idx=$(( sig + 1 ))
    REPLY="SIG${signals[${idx}]}(${sig})"
  fi
  # printf '%b' ",args:(${(j.,.)@})"
}

typeset -gA p10ks_retval

p10ks_retval "$@"
