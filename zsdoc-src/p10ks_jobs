

# AUTOGENERATED - DO NOT EDIT
# vim: ft=zsh ts=2 sw=2 et fenc=utf-8

# synopsis {{{
# Shows counts of shell jobs.
# }}}

# if no suspended jobs, only show running job count
function p10ks_jobs () {
  (( ${(Mw)#jobstates#suspended} == 0 )) && {
    REPLY="${#jobstates} ${p10ks_jobs[symbol_running]}"
  } || {
    REPLY="${(Mw)#jobstates#running}${p10ks_jobs[symbol_running]} ${(Mw)#jobstates#suspended}${p10ks_jobs[symbol_suspended]}"
  }
}

typeset -gA p10ks_jobs
# env-vars {{{
# p10ks_jobs[symbol_running] -> symbol for running jobs
p10ks_jobs[symbol_running]=${p10ks_jobs[symbol_running]:-'⚙ '}
# p10ks_jobs[symbol_suspended] -> symbol for suspended jobs
p10ks_jobs[symbol_suspended]=${p10ks_jobs[symbol_suspended]:-'❚❚'}
# }}}

p10ks_jobs "$@"

# AUTOGENERATED - DO NOT EDIT

# AUTOGENERATED - DO NOT EDIT
# vim: ft=zsh ts=2 sw=2 et fenc=utf-8

p10ks_jobs_default_opts ()
{
  reply=(cyan black CONDITIONAL normal)
}

# AUTOGENERATED - DO NOT EDIT

# AUTOGENERATED - DO NOT EDIT
# vim: ft=zsh ts=2 sw=2 et fenc=utf-8

p10ks_jobs_display_hidden ()
{
  (( ${#jobstates} == 0 )) && true || false
}

