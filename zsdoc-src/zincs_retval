

# AUTOGENERATED - DO NOT EDIT
# vim: ft=zsh ts=2 sw=2 et fenc=utf-8

# synopsis {{{
# Return value of the last command.
# }}}

# check if signal is >127, then replace with the signal name
zincs_retval ()
{
  if (( _ZINC_RETVAL_STORE == 0 )); then
    REPLY='âœ”'
  elif (( _ZINC_RETVAL_STORE <= 128 )); then
    REPLY="${_ZINC_RETVAL_STORE}"
  else
    REPLY="SIG${signals[$(( _ZINC_RETVAL_STORE - 127 ))]}($(( _ZINC_RETVAL_STORE - 128 )))"
  fi
  # printf '%b' ",args:(${(j.,.)@})"
}

typeset -gA zincs_retval

zincs_retval "$@"

# AUTOGENERATED - DO NOT EDIT

# AUTOGENERATED - DO NOT EDIT
# vim: ft=zsh ts=2 sw=2 et fenc=utf-8

zincs_retval_default_opts ()
{
  reply=(CONDITIONAL CONDITIONAL CONDITIONAL normal)
}

# AUTOGENERATED - DO NOT EDIT

# AUTOGENERATED - DO NOT EDIT
# vim: ft=zsh ts=2 sw=2 et fenc=utf-8

function _zincs_retval_preexec () {
  zincs_retval[ac]=0
}

function _zincs_retval_async_complete () {
  zincs_retval[ac]=1
}

function zincs_retval_display_hidden () {
  (( zincs_retval[ac] )) && true || false
}

zincs_retval[ac]=1

_zinc_add_hook _zinc_preexec _zincs_retval_preexec
_zinc_add_hook _zinc_async_complete _zincs_retval_async_complete

zincs_retval_display_hidden "$@"

# AUTOGENERATED - DO NOT EDIT

# AUTOGENERATED - DO NOT EDIT
# vim: ft=zsh ts=2 sw=2 et fenc=utf-8

zincs_retval_bg ()
{
  if [[ "$_ZINC_RETVAL_STORE" == '0' ]]; then
    REPLY='black'
  else
    REPLY='red'
  fi

  # _ZINC_DBG_OUT retval_bg says: "$_ZINC_RETVAL_STORE"
}

# AUTOGENERATED - DO NOT EDIT

# AUTOGENERATED - DO NOT EDIT
# vim: ft=zsh ts=2 sw=2 et fenc=utf-8

zincs_retval_fg ()
{
  if [[ $_ZINC_RETVAL_STORE == '0' ]]; then
    REPLY='green'
  else
    REPLY='black'
  fi
}

